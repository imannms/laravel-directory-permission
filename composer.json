{
    "name": "imannms/laravel-directory-permission",
    "description": "Artisan command to set appropiate permission for storage and bootstrap/cache directory.",
    "homepage": "https://github.com/imannms/laravel-directory-permission",
    "keywords":
    [
		"laravel",
		"directory",
        "permission"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "imannms",
            "email": "socialtrens@gmail.com",
            "homepage": "https://github.com/imannms",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "laravel/framework": ">=5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.5"
    },
    "autoload": {
        "psr-4": {
            "Imannms\\LaravelDirectoryPermission\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
	"extra": {
		"laravel": {
			"providers": [
				"Imannms\\LaravelDirectoryPermission\\ServiceProvider"
			]
		}
	}
}